package edu.fju.exam.Niner;

public class Niner {
	
	public static void main(String[] args) {
		for(int b = 1;b < 4;b++){
			if(b < 4 | b > 8){
				for(int a = 2;a<10;a++){
					if ((a*b)<10){
						System.out.println(a+"x"+b+"="+" "+(a*b)+"\t");
					} else {
						System.out.println(a + "x" + b + "=" + (a * b) + "\t");
					}
				}
			}

}
}
}